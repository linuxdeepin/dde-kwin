<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="com.deepin.KWin.Display.Screen">
    <property name="ColorTemperatureManual" type="i" access="read"/>
    <property name="Brightness" type="d" access="read"/>
    <property name="BestDisplayMode" type="a{sv}" access="read">
      <annotation name="org.qtproject.QtDBus.QtTypeName" value="QMap&lt;QString,QVariant&gt;"/>
    </property>
    <property name="ScreenWidth" type="i" access="read"/>
    <property name="ScreenHeight" type="i" access="read"/>
    <property name="RefreshRate" type="d" access="read"/>
    <property name="Connected" type="b" access="read"/>
    <property name="Model" type="s" access="read"/>
    <property name="Rotations" type="av" access="read"/>
    <property name="X" type="i" access="read"/>
    <property name="Y" type="i" access="read"/>
    <property name="ID" type="s" access="read"/>
    <property name="Enabled" type="b" access="read"/>
    <property name="PhysicalWidth" type="i" access="read"/>
    <property name="PhysicalHeight" type="i" access="read"/>
    <property name="Rotation" type="i" access="read"/>
    <property name="Name" type="s" access="read"/>
    <property name="Manufacturer" type="s" access="read"/>
    <property name="Modes" type="av" access="read"/>
    <property name="CurrentMode" type="av" access="read"/>
    <property name="CurrentScalingMode" type="s" access="read"/>
    <property name="SupportScalingMode" type="av" access="read"/>
    <method name="SetBrightness">
      <arg name="dBrightness" type="d" direction="in"/>
    </method>
    <method name="SetResolution">
      <arg name="nWidth" type="i" direction="in"/>
      <arg name="nHeight" type="i" direction="in"/>
    </method>
    <method name="SetRefreshRate">
      <arg name="dRefreshRate" type="d" direction="in"/>
    </method>
    <method name="SetRotation">
      <arg name="nRotation" type="i" direction="in"/>
    </method>
    <method name="SetColorTemperature">
      <arg name="nColorTemperature" type="i" direction="in"/>
    </method>
    <method name="SetScreenPosition">
      <arg name="x" type="i" direction="in"/>
      <arg name="y" type="i" direction="in"/>
    </method>
    <method name="setScalingMode">
      <arg name="nMode" type="i" direction="in"/>
    </method>
  </interface>
</node>
