    <method name="setProhibitScreenShot">
      <arg type="i" direction="out"/>
      <arg name="prohibit" type="b" direction="in"/>
    </method>
    <method name="setScreenWatermark">
      <arg name="strPolicy" type="s" direction="in"/>
    </method>
    <method name="watermarkStatus">
      <arg type="b" direction="out"/>
    </method>
    <method name="initConfig">
    </method>
  </interface>
</node>
